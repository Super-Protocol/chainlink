# PostgreSQL
PGUSER=postgres
PGPASSWORD=mysecretpassword
# Will be generated for Node 1: chainlink_node_1
PGDATABASE=chainlink_node
PGPORT=5432

# Chainlink API Credentials
CHAINLINK_EMAIL=admin@example.com
# Password should be more than 15 symbols. Example for Node 2: yoursuperpassword2
CHAINLINK_PASSWORD=yoursuperpassword

# Chainlink Configuration
# Password should be more than 15 symbols. Example for Node 2: yourkeystorepassword2
CHAINLINK_KEYSTORE_PASSWORD=yourkeystorepassword
CHAINLINK_CHAIN_ID=5611
CHAINLINK_NODE_NAME="opbnb-testnet"
CHAINLINK_RPC_WS_URL=wss://opbnb.testnet.superprotocol.com/
CHAINLINK_RPC_HTTP_URL=https://opbnb.testnet.superprotocol.com/
CHAINLINK_GAS_PRICE=10000000000
TOTAL_NODES="5"
NODES_LIST="1 2 3 4 5"
PRIMARY_NODES="1 2 3 4 5"
